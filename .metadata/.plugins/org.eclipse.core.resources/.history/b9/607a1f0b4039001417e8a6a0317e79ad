 * 
 */

/**
 * @author ruthcanavan
 *
 */
public class Calculator {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// declare variables
		double F;
		double C;
		
		//compute C
	    F = 100;
		C = (F-32)*(5.0/9.0);
		
		//print temperature in C 
		System.out.print(C);
		System.out.println(" Celsius");
		
		//compute F
		C = 10;
		F = (9.0/5.0)*C+32;
		
		//print temperature in F
		System.out.print(F);
		System.out.print(" Fahrenheit");

		
	}

}
