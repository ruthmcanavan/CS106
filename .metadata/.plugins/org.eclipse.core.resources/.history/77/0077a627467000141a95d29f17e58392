/**
 * @author ruthcanavan
 *
 */
public class recursionPractice {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	counter(10);
	int i = 0;	
	}
	public static void counter(int max, int i) {
		
		i++;
		
		if (i<10) {
			System.out.print(i + " ");
			counter(10, i);
		}else{
			System.out.println(max);
			
	}
		
	
	}
}