 */
public class recursionPractice {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	int i = 0;	
	System.out.println(i);	
		
	}
	public static void counter(int i) {
		
		i++;
		if (i<10) {
			System.out.println(i);
		}else{
			System.out.println("10");
		
	}

	
}
}