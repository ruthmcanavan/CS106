/**
 * 
 */

/**
 * @author ruthcanavan
 *
 */
public class recursionPractice {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	counter(10);
		
	}
	public static void counter(int max) {
		int i = 0;
		i++;
		
		if (i<10) {
			System.out.print(i + " ");
			counter(i);
		}else{
			System.out.println(max);
			
	}
		
	
	}
}